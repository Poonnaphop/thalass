    "deploy": "gh-pages -d build",
    "copy-fonts": "node -e \"const fs = require('fs'); const path = require('path'); const src = path.join(process.cwd(), 'public/fonts/vfs_fonts.js'); const dest = path.join(process.cwd(), 'node_modules/pdfmake/build/vfs_fonts.js'); fs.copyFileSync(src, dest); console.log('Fonts copied successfully!');\"",
